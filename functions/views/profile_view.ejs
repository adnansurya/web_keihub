<!DOCTYPE html>
<html>
<head>
	<%- include('all/meta'); -%>
	<title>KEIHUB</title>
	<%- include('all/css'); -%>
</head>
<body class="skin-all">
	<header class="primary">

		<%- include('all/header'); -%>
		<!-- Start nav -->
		<%- include('all/nav'); -%>
		<!-- End nav -->
	</header>

	<section class="home">
		<div class="container">
			<h2>Profil Saya</h2>
			<br>
			<h6>Info Umum</h6>
			<img src="http://www.personalbrandingblog.com/wp-content/uploads/2017/08/blank-profile-picture-973460_640.png" width="100px" height="100" alt="Default Profile Picture">
			<br>
			<form>
				<input type="file" name="profilepicture_pic" accept="image/jpeg" multiple disabled>
				<p>Format Profile adalah .jpeg dengan ukuran 100 x 100 pixels</p>
			</form>
			<br>
			<label>Nama Lengkap :</label>
			<div class="form-group">
				<input class="form-control rounded" type="text" disabled>
			</div>
			<label>Nama Pengguna :</label>
			<div class="form-group">
				<input class="form-control rounded" type="text" disabled>
			</div>
			<label>Jenis Kelamin :</label>
			<form>
				<div class="iradio_square" style="position: relative;">
					<input type="radio" name="jeniskelamin" value="laki-laki" disabled> Laki-Laki<br>
					<input type="radio" name="jeniskelamin" value="perempuan" disabled> Perempuan<br>
				</div>
			</form>
			<br>
			<label>Tanggal Lahir :</label>
			<div class="form-group">
				<input class="form-control rounded" type="date" disabled>
			</div>
			<label>Nomor Telepon :</label>
			<div class="form-group">
				<input class="form-control rounded" type="number" disabled>
			</div>
			<label>Alamat Lengkap :</label>
			<div class="form-group">
				<input class="form-control rounded" type="text" disabled>
			</div>
			<label>Bio Singkat</label>
			<div class="form-group floating stacked">
<textarea class="ckeditor" id="ckeditor" disabled></textarea>
			</div>
			<br>
			<a class="btn btn-success" href="profile_edit">Edit Profil</a>
		</div>
	</section>

		<!-- Start footer -->
    <%- include('all/footer'); -%>
		<!-- End Footer -->

		<!-- JS -->
    <%- include('all/script'); -%>
	</body>
</html>
